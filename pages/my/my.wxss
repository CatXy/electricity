.myface{
  width: 100%;
  height: 200rpx;
  background: #e54847;
  display: flex;
  align-items: center;
}
.face{
  width: 150rpx;
  height: 150rpx;
  margin:25rpx 50rpx;
  border-radius: 50%;
  box-shadow: 3px 3px 5px rgba(0,0,0,.3);
}
.name{
  color: #fff;
  font-size: 50rpx;
  font-family: "微软雅黑";
  text-shadow: 6rpx 6rpx 8rpx #000;
}
.btnLogin{
  /* background: none; */
  background: transparent;
  /* background: rgba(0,0,0,0); */
  border: 1px solid #fff;
  color: #fff;
  border-radius: 100rpx;
}
.weui-cell__icon{
  width: 20px;
  height: 20px;
  vertical-align: middle;
  margin-right: 16px;
}
.btnKF{
  width: 100% !important;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background: transparent;
}
